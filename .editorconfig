# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

# 2 space indentation
[*.yml]
indent_style = space
indent_size = 2

# 2 space indentation
[*.tf]
indent_style = space
indent_size = 2

# Ignore paths
[**/__pycache__/**]
charset = unset
end_of_line = unset
insert_final_newline = unset
trim_trailing_whitespace = unset
indent_style = unset
indent_size = unset
